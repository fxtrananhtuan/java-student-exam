<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <application>        
        <view-handler>org.ajax4jsf.application.AjaxViewHandler</view-handler>
    </application>
    
    <navigation-rule>
        <from-view-id>/template/templateUser.jsp</from-view-id>
        <navigation-case>
            <from-outcome>user</from-outcome>
            <to-view-id>/template/templateUser.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/template/loginStudent.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>mBLogin</managed-bean-name>
        <managed-bean-class>userPack.MBLogin</managed-bean-class>  
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>mBTempAdmin</managed-bean-name>
        <managed-bean-class>helperPack.MBTempAdmin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/templateAdmin/templateAdmin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/templateAdmin/templateAdmin.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/templateAdmin/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>mBLoginAdmin</managed-bean-name>
        <managed-bean-class>adminPack.MBLoginAdmin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/templateAdmin/login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/templateAdmin/templateAdmin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>mBChangePass</managed-bean-name>
        <managed-bean-class>adminPack.MBChangePass</managed-bean-class>  
        <managed-bean-scope>request</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>checkSession</managed-bean-name>
        <managed-bean-class>helperPack.checkSession</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBAccountManager</managed-bean-name>
        <managed-bean-class>adminPack.MBAccountManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbS</managed-bean-name>
        <managed-bean-class>adminPack.mbStudentControl</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBTest</managed-bean-name>
        <managed-bean-class>helperPack.MBTest</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/test.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/templateAdmin/templateAdmin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>mBAssignmentManager</managed-bean-name>
        <managed-bean-class>adminPack.MBAssignmentManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBDownLoadFile</managed-bean-name>
        <managed-bean-class>helperPack.MBDownLoadFile</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBRegisterStudentForAssignment</managed-bean-name>
        <managed-bean-class>adminPack.MBRegisterStudentForAssignment</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBRegisterSurvey</managed-bean-name>
        <managed-bean-class>adminPack.MBRegisterSurvey</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBFileUploadManager</managed-bean-name>
        <managed-bean-class>adminPack.MBFileUploadManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBUserAssignment</managed-bean-name>
