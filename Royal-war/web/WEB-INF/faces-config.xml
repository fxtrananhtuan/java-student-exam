        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbCourseController</managed-bean-name>
        <managed-bean-class>adminPack.mbCourseController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbFAQController</managed-bean-name>
        <managed-bean-class>adminPack.mbFAQController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbMarkStudentController</managed-bean-name>
        <managed-bean-class>adminPack.mbMarkStudentController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbSemesterController</managed-bean-name>
        <managed-bean-class>adminPack.mbSemesterController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbSubjectController</managed-bean-name>
        <managed-bean-class>adminPack.mbSubjectController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbLoginStudent</managed-bean-name>
        <managed-bean-class>userPack.mbLoginStudent</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mbForgotPass</managed-bean-name>
        <managed-bean-class>userPack.mbForgotPass</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sendEmail</managed-bean-name>
        <managed-bean-class>helperPack.sendEmail</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>mBUserFileUploading</managed-bean-name>
        <managed-bean-class>userPack.MBUserFileUploading</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>kiemtraBM</managed-bean-name>
        <managed-bean-class>userPack.kiemtraBM</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/template/loginStudent.jsp</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/template/templateUser.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>mBTempUser</managed-bean-name>
        <managed-bean-class>helperPack.MBTempUser</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
